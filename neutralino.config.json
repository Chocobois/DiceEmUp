{
  "applicationId": "fireanddice",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/public/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.exit",
    "os.showMessageBox"
  ],
  "globalVariables": {
    "isNeutralino": true
  },
  "modes": {
    "window": {
      "title": "Fire and Dice",
      "width": 960,
      "height": 540,
      "minWidth": 960,
      "minHeight": 540,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/public/icon.png",
      "enableInspector": false,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": false,
      "globalVariables": {
        "isNeutralino": true
      }
    }
  },
  "cli": {
    "binaryName": "FireAndDice",
    "resourcesPath": "/public/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/static/neutralino.js",
    "binaryVersion": "4.6.0",
    "clientVersion": "3.5.0"
  }
}